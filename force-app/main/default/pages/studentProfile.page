        function populateStudentProfile(result) {
            try{
            console.log("Populating Student Profile");
            allowPanelistToAddDetails(assignedPanelistRoundNumber);
            const studentAssessmentList = result.studentAssessmentList;

            displayProfileImage(result.studentDetails.student_image__c, result.studentDetails.Gender__c);

            let round2instance = 0;
            let round3instance = 0;
            let round4instance = 0;

            if (Array.isArray(studentAssessmentList) && studentAssessmentList.length > 0) {

                const assessmentsMap = new Map();

                for (const assessment of studentAssessmentList) {
                    const round = parseInt(assessment.round);
                    const instanceId = assessment.instanceId;
                    if (!assessmentsMap.has(round)) {
                        assessmentsMap.set(round, new Map());
                    }
                    assessmentsMap.get(round).set(instanceId, assessment);
                }

                assessmentsMap.forEach((instanceMap, round) => {
                    instanceMap.forEach((assessmentForThisRound, instanceId) => {

                        let parentDiv = document.getElementById(`round${round}`);
                        if( round == '1'){parentDiv.classList.add("hidden");}
                        console.log("Searching: " + parentDiv + " By querying " + `round${round}`);
                        if (!parentDiv) {
                            console.error(`Div for round ${round} and instance ${instanceId} not found.`);
                        }

                        if (instanceId) {
                            const cloneDiv = parentDiv.cloneNode(true);
                            cloneDiv.classList.remove('hidden');
                            cloneDiv.id = `round${round}Alt${instanceId - 1}`;
                            parentDiv.parentNode.insertBefore(cloneDiv, parentDiv.nextSibling);

                            cloneDiv.querySelector('#editButton').setAttribute('onclick', `enableEdit('round${round}Alt${instanceId - 1}','testEdit')`);
                            // cloneDiv.querySelector('#addElementButton').setAttribute('onclick', `addNewElement('round${round}Alt${instanceId - 1}')`);
                            cloneDiv.querySelector('#saveButton').setAttribute('onclick', `saveStudent('round${round}Alt${instanceId - 1}')`);
                            
                            parentDiv = cloneDiv;
                        }
                        //console.log("Current Panelist is " + panelistId + " The saved panelist " + assessmentForThisRound.panelistId );
                        if (panelistId === assessmentForThisRound.panelistId){parentDiv.querySelector("#editButton").classList.remove("hidden");}
                        if (round == '2'){
                            round2instance++;
                            parentDiv.querySelector('#instanceId').textContent = assessmentForThisRound.instanceId;
