        function enableEdit(editCard, testEdit) {
            

            const parentElement = eval(editCard);
            // console.log( "The Card To Edit is " + parentElement);
            const findRoundNum = (parentElement instanceof HTMLDivElement) ? parentElement.id : parentElement.toString();

            if(findRoundNum.includes('2'))
            {
                document.getElementById('round2Err').classList.add('hidden');
            }
            else if (findRoundNum.includes('3'))
            {
                document.getElementById('round3Err').classList.add('hidden');
            }
            else if (findRoundNum.includes('4'))
            {
                document.getElementById('round4Err').classList.add('hidden');
            }

            console.log("Parent Element "  + parentElement);
            console.log("Allow Editing "+testEdit);

            if(testEdit === "testEdit"){
                console.log("Instance ID of the Editing Card "+parentElement.querySelector('#instanceId').textContent);
                editedFormData.instanceId = parentElement.querySelector('#instanceId').textContent;
            }
            if (parentElement) {
                const btnGroup = parentElement.querySelector('.button-group');
                if (btnGroup) {
                    btnGroup.classList.add('hidden');
                }

                const opButton = parentElement.querySelector('.operation-buttons');
                if (opButton) {
                    opButton.classList.remove('hidden');
                }
                const panelistName = parentElement.querySelector("#panelistFormId");
                panelistName.textContent =  assignedPanelistName;
                editedFormData.panelistName = assignedPanelistName;
                editedFormData.panelistId = panelistId;
                //Cancel Button, Unfinished, for now im just refreshing the window.
                // const cancelButton = document.createElement('div');
                // cancelButton.className = 'cancel-button';
                // cancelButton.textContent = 'Cancel';
                // cancelButton.addEventListener('click', function () {
                //     location.reload();
                // });

                // parentElement.appendChild(cancelButton);

                const editVar = parentElement.querySelectorAll('.editable');
                for (const element of editVar) {
                    if (element.classList.contains('option')) {
                        const selectElement = document.createElement('select');
                        selectElement.id = element.id;
                        const defaultOption = document.createElement('option');

                        const options = document.createElement('optgroup');
                        options.label = 'Select an Option';

                        if (element.classList.contains('student-status')) {
                            const studentStatusOptions = ['Approved', 'On-Hold', 'Rejected'];
                            studentStatusOptions.forEach(optionText => {
                                const option = document.createElement('option');
                                option.value = optionText;
                                option.text = optionText;
                                options.appendChild(option);
                            });
                        } 
                        else if (element.classList.contains('yes-or-no')) {
                            const studentStatusOptions = ['Yes','No'];
                            studentStatusOptions.forEach(optionText => {
                                const option = document.createElement('option');
                                option.value = optionText;
                                option.text = optionText;
                                options.appendChild(option);
                            });
                        } 
                        else {
                            const otherOptions = ['Excellent', 'Good', 'Fair', 'Poor'];
                            otherOptions.forEach(optionText => {
                                const option = document.createElement('option');
                                option.value = optionText;
                                option.text = optionText;
                                options.appendChild(option);
                            });
                        }

                        selectElement.appendChild(options);
                        selectElement.value = element.textContent;

                        selectElement.addEventListener('change', function (event) {
                            const editedValue = event.target.value;
                            editedFormData[element.id] = editedValue;
                        });

                        editedFormData[element.id] = element.textContent;
                        // editefFormData[]
                        element.parentNode.replaceChild(selectElement, element);
                    }
                    if (element.classList.contains('comment')) {
                        const textareaElement = document.createElement('textarea');
                        textareaElement.type = 'text';
                        textareaElement.id = element.id;
                        textareaElement.value = element.textContent;

                        textareaElement.addEventListener('input', function (event) {
                            const editedValue = event.target.value;
                            editedFormData[element.id] = editedValue;
                        });

                        editedFormData[element.id] = element.textContent;
                        element.parentNode.replaceChild(textareaElement, element);
                    }
                }
            }
        }
        function saveStudent(editCard) {
            // showLoader();
            console.log("The Edited Form Data is " + editedFormData);
            if (typeof editedFormData === 'undefined') {
                console.error('editedFormData is not defined');
                return;
            }

            let roundNumber = null;
            let jsonList = [];

            if (typeof editCard === 'string' || (editCard instanceof HTMLDivElement && editCard.id)) {
                const editCardString = (editCard instanceof HTMLDivElement) ? editCard.id : editCard.toString();

                if (editCardString.includes('round1')) {
                    roundNumber = '1';
                } else if (editCardString.includes('round2')) {
                    roundNumber = '2';
                } else if (editCardString.includes('round3')) {
                    roundNumber = '3';
                } else if (editCardString.includes('round4')) {
                    roundNumber = '4';
                }
            }

            if (roundNumber !== null) {
                editedFormData.round = roundNumber;
                let maxInstanceId = 1;
                if (Array.isArray(savedResult)) {
                    console.log("Checking for instances");
                    let updated = false;

                    for (let i = 0; i < savedResult.length; i++) {
                        const item = savedResult[i];

                        console.log(parseInt(item.instanceId));
                        console.log(editedFormData.instanceId);
                        console.log("Checking all the previous Instances");

                        if (item.round === roundNumber) 
                        {
                            if (parseInt(item.instanceId) >= maxInstanceId) {
                                maxInstanceId = parseInt(item.instanceId) + 1;
                            }
                            if (parseInt(item.instanceId) === parseInt(editedFormData.instanceId)) {
                                console.log("Previous Instance Updated");
                                console.log("Previous Saved Result "+ savedResult[i]);

                                savedResult[i] = editedFormData;
                                updated = true;
                            }

                        }
                        jsonList.push(savedResult[i]);
                    }
                }

                if (!editedFormData.instanceId) {
                    console.log("Creating a new Instance");
                    editedFormData.instanceId = maxInstanceId;
                    jsonList.push(editedFormData);
                }

                //jsonList.push(editedFormData);

                const editedFormDataJSON = JSON.stringify(jsonList, null, 2).replace(/[\/\(\)\']/g, "\\$&");
                console.log("The for to save is" + editedFormDataJSON);
                showLoader();
                Visualforce.remoting.Manager.invokeAction(
                    '{!$RemoteAction.studentProfileController.updateStudentAssessment}',
                    editedFormDataJSON,
                    studentId,
                    eventId,
                    panelistId,
                    function(result, event) {
                        if (event.status) {
                            console.log("Appended Data Successfully");
                            editedFormData = {};
                            location.reload();
                        } else {
                            hideLoader();
                            displayErrorMessage('Failed to Save Details');
                            console.error('Error:', event.message);
                        }
                    }
                );
            } else {
                console.error("Invalid editCard value:", editCard);
            }
        }

        function setNewElement(roundId) {
            let maxSuffix = -1;
            let newElementId = "";

            const defaultElement = document.getElementById(roundId);
            
            if (!defaultElement) {

                const elements = document.querySelectorAll(`[id^="${roundId}alt"]`);
                
                if (elements.length === 0) {

                    newElementId = roundId;
                } else {

                    elements.forEach((element) => {
                        const suffix = parseInt(element.id.replace(`${roundId}alt`, ''));
                        if (!isNaN(suffix) && suffix > maxSuffix) {
                            maxSuffix = suffix;
                        }
                    });

                    newElementId = `${roundId}alt${maxSuffix}`;
                }
            } else {

                newElementId = roundId;
            }

            console.log("Selected ID:", newElementId);
            addNewElement(newElementId);
        }



        function addNewElement(editCard) {
            console.log("Existing Element Sent to addNewElement "+editCard)
            const existingDiv = document.getElementById(editCard);

            if (existingDiv) {
                const altSuffix = 'Alt';
